doctype html
html(ng-app="mApp")
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='theme-color' content='#4DA5F4')
    meta(name='csrf-token', content=_csrf)
    title #{title} - Building Nrg
    link(rel='shortcut icon', href='/favicon.png')
    link(rel='stylesheet', href='/css/main.css')
    link(rel='stylesheet', type='text/css', href='bootstrap/css/bootstrap.css')
    link(rel='stylesheet', type='text/css', href='bootstrap/css/bootstrap-responsive.min.css')
    link(rel='stylesheet', type='text/css', href='stylesheets/style.css')
    link(rel='stylesheet', type='text/css', href='bootstrap/bootselect/dist/css/bootstrap-select.css')   
    

  body
    include partials/header

    .container
      include partials/flash
      block content

    include partials/footer

    script(type='text/javascript', src='javascript/jquery-3.1.1.min.js')
    //script(src='/js/lib/bootstrap.min.js')
    script(src='javascript/main.js')
    script(type='text/javascript', src='bootstrap/js/bootstrap.min.js')
    script(type='text/javascript', src='bootstrap-datepicker-master/js/bootstrap-datepicker.js')
    script(type='text/javascript', src='bootstrap/bootselect/dist/js/bootstrap-select.js')
    script(type='text/javascript', src="javascript/highcharts.js")

